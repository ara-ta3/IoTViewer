{
  "name": "HueViewer",
  "version": "0.0.4",
  "description": "",
  "main": "__dist/main.js",
  "build": {
    "appId": "com.electron.hueviewer",
    "directories": {
      "output": "__build"
    },
    "files": [
      "__dist"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": {
        "target": "zip",
        "arch": [
          "x64"
        ]
      }
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "scripts": {
    "electron": "electron .",
    "build": "webpack",
    "build-watch": "webpack -w",
    "electron-build-mac": "electron-builder --mac --x64",
    "electron-build-mac-app": "electron-builder --mac --x64 --dir",
    "electron-build-win": "electron-builder --win --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ara-ta3/HueViewer.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ara-ta3/HueViewer/issues"
  },
  "homepage": "https://github.com/ara-ta3/HueViewer#readme",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.12",
    "@types/react-redux": "^7.1.23",
    "electron": "^13.3.0",
    "electron-builder": "^22.14.13",
    "prettier": "^2.5.1",
    "ts-loader": "^9.2.7",
    "typescript": "^4.6.2",
    "webpack": "5.69.1",
    "webpack-cli": "4.9.2"
  },
  "dependencies": {
    "@emotion/react": "^11.8.1",
    "@emotion/styled": "^11.8.1",
    "@mui/icons-material": "^5.4.4",
    "@mui/material": "^5.4.4",
    "@mui/styled-engine": "^5.4.4",
    "@types/node-fetch": "^2.6.1",
    "electron-store": "^8.0.1",
    "fp-ts": "^2.11.8",
    "node-fetch": "^2.6.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.1"
  }
}
