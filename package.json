{
  "name": "HueViewer",
  "version": "0.0.4",
  "description": "",
  "main": "__dist/main.js",
  "build": {
    "appId": "com.electron.hueviewer",
    "directories": {
      "output": "__build"
    },
    "files": [
      "__dist"
    ],
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": {
        "target": "zip",
        "arch": [
          "x64"
        ]
      }
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "scripts": {
    "electron": "electron .",
    "build": "webpack",
    "build-watch": "webpack -w",
    "electron-build-mac": "electron-builder --mac --x64",
    "electron-build-mac-app": "electron-builder --mac --x64 --dir",
    "electron-build-win": "electron-builder --win --x64"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ara-ta3/HueViewer.git"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ara-ta3/HueViewer/issues"
  },
  "homepage": "https://github.com/ara-ta3/HueViewer#readme",
  "devDependencies": {
    "@types/electron": "^1.6.10",
    "@types/node": "^20.8.6",
    "@types/react": "^18.2.28",
    "@types/react-dom": "^18.2.13",
    "@types/react-redux": "^7.1.27",
    "electron": "27.0.0",
    "electron-builder": "^24.6.4",
    "prettier": "^3.0.3",
    "ts-loader": "^9.5.0",
    "typescript": "^5.0.4",
    "webpack": "5.88.2",
    "webpack-cli": "5.1.4"
  },
  "dependencies": {
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.14.13",
    "@mui/material": "^5.14.13",
    "@mui/styled-engine": "^5.14.12",
    "@types/node-fetch": "^2.6.4",
    "electron-store": "^8.1.0",
    "fp-ts": "^2.16.1",
    "node-fetch": "^2.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2"
  }
}
